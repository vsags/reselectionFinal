<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane maxWidth="300.0" minWidth="170.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="reselection.comparison.AddEquipmentController">
   <center>
      <BorderPane prefWidth="142.0">
         <top>
            <VBox fx:id="addDevicesBox" alignment="TOP_RIGHT" prefHeight="460.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <VBox fx:id="noDevicesBox" alignment="CENTER" prefHeight="460.0" prefWidth="170.0">
                     <children>
                        <Label text="Других устройств">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <Label text="с похожими">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <Label text="характеристиками">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <Label text="нет">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                     </children>
                  </VBox>
               </children></VBox>
         </top>
         <bottom>
            <VBox fx:id="controlButtonsBox" alignment="CENTER" prefHeight="100.0" prefWidth="142.0" spacing="8.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="closeButton" mnemonicParsing="false" onAction="#closeAddEquipmentView" prefWidth="80.0" text="Закрыть">
                     <font>
                        <Font size="14.0" />
                     </font></Button>
               </children>
            </VBox>
         </bottom>
      </BorderPane>
   </center>
</BorderPane>
